<!--pages/teacher/classlist/completeddetail/namelist.wxml-->
<view style="width:100%;height:100rpx;display:flex;border-bottom:2rpx solid #e5e5e5">
	<view style="width:20%;font-size:30rpx;color:#FFCC1A;line-height:100rpx;text-align:center">姓名</view>
	<view style="width:40%;font-size:30rpx;color:#FFCC1A;line-height:100rpx;text-align:center">名称</view>
	<view style="width:40%;font-size:30rpx;color:#FFCC1A;line-height:100rpx;text-align:center">时间</view>
</view>
<block wx:for="{{web_mainData}}" wx:key="id">
	<view style="width:100%;height:100rpx;display:flex;border-bottom:2rpx solid #e5e5e5">
		<view style="width:20%;font-size:30rpx;color:#2e2e2e;line-height:100rpx;text-align:center">{{item.user_info.name}}</view>
		<view style="width:40%;font-size:30rpx;color:#2e2e2e;line-height:100rpx;text-align:center">{{item.product.title}}</view>
		<view style="width:40%;font-size:30rpx;color:#2e2e2e;line-height:100rpx;text-align:center">{{item.create_time}}</view>
	</view>
</block>



